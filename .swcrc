{
	"$schema": "https://json.schemastore.org/swcrc",

	"jsc": {
		"parser": {
			"syntax": "typescript",
			"dynamicImport": true
		},

		"target": "es5"
	},
	"minify": false,
	"module": {
		"type": "commonjs",

		// These are defaults.
		"strict": false,
		"strictMode": true,
		"lazy": false,
		"noInterop": false
	},
	"sourceMaps": true
}
